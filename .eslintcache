  <input\n          name=\"password\"\n          type=\"password\"\n          placeholder=\"Password\"\n          required\n          value={password}\n          onChange={onChange}\n        />\n        <input type=\"submit\" value={newAccount ? \"Create Account\" : \"Log in\"} />\n      </form>\n      <span onClick={toggleAccount}>\n        {newAccount ? \"Sign In\" : \"Create Account\"}\n      </span>\n      <div>\n        <button name=\"google\" onClick={(e) => onClickSocial(e)}>\n          Continue with Google\n        </button>\n        <button name=\"github\" onClick={(e) => onClickSocial(e)}>\n          Continue with Github\n        </button>\n      </div>\n    </div>\n  );\n};\n\nexport default Auth;\n","/Users/seodolong/workspace/nwitter/src/routes/Home.js",[],"/Users/seodolong/workspace/nwitter/src/components/AppRouter.js",[],"/Users/seodolong/workspace/nwitter/src/fBase.js",[],"/Users/seodolong/workspace/nwitter/src/components/Navigation.js",[],"/Users/seodolong/workspace/nwitter/src/routes/Profile.js",["41"],"import React from \"react\";\nimport { authService } from \"fBase\";\nimport { useHistory } from \"react-router-dom\";\n\nexport default () => {\n  const history = useHistory();\n\n  const onLogOutClick = () => {\n    authService.signOut();\n    history.push(\"/\");\n  };\n\n  return (\n    <>\n      <button onClick={onLogOutClick}>Log Out</button>\n    </>\n  );\n};\n",{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","severity":1,"message":"47","line":8,"column":10,"nodeType":"48","messageId":"49","endLine":8,"endColumn":15},{"ruleId":"46","severity":1,"message":"50","line":50,"column":9,"nodeType":"48","messageId":"49","endLine":50,"endColumn":13},{"ruleId":"51","severity":1,"message":"52","line":5,"column":1,"nodeType":"53","endLine":18,"endColumn":3},"no-native-reassign",["54"],"no-negated-in-lhs",["55"],"no-unused-vars","'error' is assigned a value but never used.","Identifier","unusedVar","'data' is assigned a